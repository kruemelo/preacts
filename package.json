 {
  "name": "@kruemelo/preacts",
  "version": "1.0.0-alpha21",
  "description": "preact standalone",
  "keywords": [
    "react",
    "preact",
    "standalone",
    "browser"
  ],
  "exports":  {
    "types": "./dist/preacts.d.ts"
  },
  "types": "./dist/preacts.d.ts",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rm -f package-lock.json && npm update && npm run build:lib && npm run build:cp-lib",
    "build:lib": "microbundle ./src/preacts.ts -o ./preacts.js -f modern --no-pkg-main --target web",
    "build:cp-lib": "cp ./preacts.js ./dist/preacts.js && cp ./preacts.js.map ./dist/preacts.js.map",
    "xbuild:types": "tsc "
  },
  "author": "kruemelo",
  "homepage": "https://github.com/kruemelo/preacts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruemelo/preacts.git"
  },
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "@preact/signals": "^1.2.2",
    "htm": "^3.1.1",
    "microbundle": "^0.15.1",
    "preact-custom-element": "^4.3.0",
    "wouter-preact": "^3.0.0"
  }
}

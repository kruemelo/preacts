{
  "name": "@kruemelo/preacts",
  "version": "1.0.0-alpha8",
  "description": "preact standalone",
  "keywords": [
    "react",
    "preact",
    "standalone",
    "browser"
  ],
  "exports": "./index.js",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
